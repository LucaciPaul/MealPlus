<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="lucacipaul.mealplus">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".frontend.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".frontend.RegisterAdviser" />
        <activity android:name=".frontend.RegisterAdviserFinal" />
        <activity android:name=".frontend.RegisterCustomer" />
        <activity android:name=".frontend.RegisterCustomerFinal" />
        <activity android:name=".frontend.RegisterUser" />
        <activity android:name=".frontend.CustomerDashboard" />
    </application>

</manifest>